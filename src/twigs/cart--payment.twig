
{% set data = {
  pageId: 'cart',
  pageTheme: 'orange-sun',
  pageTitle: 'Your Cart',
  subpageId: 4,
} %}

{% extends 'core/template.twig' %}

{% block Content %}

  {{ Header({
    headline: 'Your Cart',
    text: 'You have no items yet',
  }) }}

  {{ Subnavigation({
    pageId: data.pageId,
  }) }}

  {{ Title({
    border: true,
    headline: 'Payment',
  }) }}

  <div class="unicorn">
    {{ source('src/assets/images/unicorn.svg')|raw }}
