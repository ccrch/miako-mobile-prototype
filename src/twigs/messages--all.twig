
{% set data = {
  pageId: 'messages',
  pageTheme: 'blue-sky',
  pageTitle: 'Messages',
  subpageId: 1,
} %}

{% extends 'core/template.twig' %}

{% block Content %}

  {{ Header({
    headline: 'Messages',
    text: 'You\'ve got <em>3</em> new messages',
  }) }}

  {{ Subnavigation({
    pageId: data.pageId,
  }) }}

  {{ Headline({
    headline: 'List of all your messages',
  }) }}

  <div class="unicorn">
    {{ source('src/assets/images/unicorn.svg')|raw }}
  </div>

{% endblock %}
